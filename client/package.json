{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "npm install --legacy-peer-deps && vite build",
    "build:check": "node -e \"const env = require('dotenv').config(); const required = ['VITE_API_URL', 'VITE_FIREBASE_API_KEY', 'VITE_RAZORPAY_KEY_ID']; const missing = required.filter(k => !process.env[k]); if(missing.length) { console.error('Missing:', missing); process.exit(1); }\"",
    "prebuild": "npm run build:check || true",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "install:prod": "npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@sentry/react": "^10.26.0",
    "@sentry/vite-plugin": "^4.6.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/vite": "^4.0.13",
    "axios": "^1.8.3",
    "chart.js": "^4.4.9",
    "firebase": "^11.4.0",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.541.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.3.0",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^24.0.0",
    "vite": "^6.2.1",
    "vitest": "^2.1.8"
  }
}
